<% for post in @posts.models: %>
  <div class="post" id="post-<%= post.get('id') %>">
    <h1><a href="/posts/<%= post.get('id') %>"><%= post.get('title') %></a></h1>
    <h3><%= post.get('post_date') %></h3>
    <%= console.log(post.get('post_date'))%>
  </div>
  <div>
    <%- post.get('content') %>
    <p class="elipse" ><strong>...</strong></p>
  </div>
<% end %>
<% if @posts.models.length == 0: %>
  <h1>No posts yet...</h1>


