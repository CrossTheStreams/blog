<div class="post" id="post-<%= @post.get('id') %>">
    <h1><a href="/posts/<%= @post.get('id') %>"><%= @post.get('title') %></a></h1>
    <div class="tags">
      <strong>Tags:</strong>
      <%= @post.attributes.keywords.map (d) -> " " + d %>
    </div>
    <h3><%= @post.get('date_published') %></h3>
    <div><%- @post.get('content') %></div>
</div>





